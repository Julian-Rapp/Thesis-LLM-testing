@misc{raoCATLMTrainingLanguage2023,
  title = {{{CAT-LM}}: {{Training Language Models}} on {{Aligned Code And Tests}}},
  shorttitle = {{{CAT-LM}}},
  author = {Rao, Nikitha and Jain, Kush and Alon, Uri and Goues, Claire Le and Hellendoorn, Vincent J.},
  year = {2023},
